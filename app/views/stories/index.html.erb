<h2>be open..... be freeee......</h2>

<% if @stories.any? %>
  <ul class="list-unstyled">
    <% @stories.each do |story| %>
      <li><%= link_to story.title, story_path(story)%></li>
    <% end %>
  </ul>
<% else %>
  <h3>No Stories Yet</h3>
<% end %>
